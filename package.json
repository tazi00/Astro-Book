{
  "name": "astrobook",
  "private": true,
  "packageManager": "pnpm@10.28.0",
  "scripts": {
    "build": "tsc -b",
    "dev:server": "pnpm --filter server dev",
    "dev:web": "pnpm --filter web dev",
    "dev:mobile": "pnpm --filter mobile start"
  },
  "pnpm": {
    "overrides": {
      "react": "19.1.0",
      "react-dom": "19.1.0"
    },
    "packageExtensions": {
      "expo-keep-awake": {
        "dependencies": {
          "react": "19.1.0"
        }
      }
    }
  }
}
